	<TextView
		android:text="Sl.No"
		android:textSize="18sp" android:gravity="center"
		android:layout_width="60sp"
		android:layout_height="40sp"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="70sp"
		android:layout_height="wrap_content">
	<TextView
		android:text="Ev.Code"
		android:layout_width="70sp" android:textSize="18sp"
		android:gravity="center"
		android:layout_height="40sp"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="120sp"
		android:layout_height="wrap_content">
	<TextView android:layout_column="4"
		android:text="Ev.Date"
		android:layout_width="120sp" android:textSize="18sp"
		android:gravity="center"
		android:layout_height="40sp"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="100sp"
		android:layout_height="wrap_content">
	<TextView android:layout_column="5"
		android:text="Info-1"
		android:layout_width="100sp" android:textSize="18sp"
		android:gravity="center"
		android:layout_height="40sp"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="70sp"
		android:layout_height="wrap_content">
	<TextView android:layout_column="6"
		android:text="Info-2"
		android:layout_width="70sp" android:textSize="18sp"
		android:gravity="center"
		android:layout_height="40sp"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="70sp"
		android:layout_height="wrap_content">
	<TextView android:layout_column="7"
		android:text="Info-3"
		android:layout_width="70sp" android:textSize="18sp"
		android:gravity="center"
		android:layout_height="40sp"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="120sp"
		android:layout_height="wrap_content">
	<TextView android:layout_column="8"
		android:text="Info-4"
		android:layout_width="120sp" android:textSize="18sp"
		android:gravity="center"
		android:layout_height="40sp"/>
	</LinearLayout>
	<LinearLayout
		android:layout_width="70sp"
		android:layout_height="wrap_content">
	<TextView android:layout_column="9"
		android:text="Round"
		android:layout_width="70sp" android:textSize="18sp"
		android:gravity="center"
		android:layout_height="30sp"/>
	</LinearLayout>
