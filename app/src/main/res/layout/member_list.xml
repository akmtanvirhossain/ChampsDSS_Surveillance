            android:text="গর্ভের ইতিহাস"
            android:textSize="16sp" />

        <Button
            android:id="@+id/cmdEvList"
            android:layout_width="20sp"
            android:layout_height="match_parent"
            android:layout_marginBottom="5sp"
            android:layout_marginLeft="10sp"
            android:layout_marginTop="5sp"
            android:layout_weight="1"
            android:background="@drawable/button_style"
            android:paddingLeft="10dp"
            android:text="ঘটনার তালিকা"
            android:textSize="16sp" />

        <Button
            android:id="@+id/cmdVisitList"
            android:layout_width="20sp"
            android:layout_height="match_parent"
            android:layout_marginBottom="5sp"
            android:layout_marginLeft="10sp"
            android:layout_marginTop="5sp"
            android:layout_weight="1"
            android:background="@drawable/button_style"
            android:text="পরিদর্শন তালিকা"
            android:textSize="16sp" />

    </LinearLayout>

    <View android:id="@+id/lineMenu" android:layout_height="1dip" android:layout_width="fill_parent" android:layout_marginBottom="0sp" android:background="#CCCCCC" />

    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:layout_marginBottom="0sp"
                android:background="#CCCCCC" />

            <include
                layout="@layout/memberheading"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:layout_marginBottom="0sp"
                android:background="#CCCCCC" />

            <ScrollView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:isScrollContainer="true"
                android:overScrollMode="always"
                android:scrollbarAlwaysDrawVerticalTrack="true"
                android:scrollbarStyle="outsideInset"
                android:scrollbars="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="0sp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/secListView"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ListView
                            android:id="@+id/lstData"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:divider="#000000"
                            android:dividerHeight="1px"
                            android:scrollbars="vertical"></ListView>
                    </LinearLayout>

                    <LinearLayout
                        style="@style/rowStyle0"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="#F3F3F3"
                        android:orientation="horizontal">
                        <TextView android:id="@+id/VlblReview" android:text="R-তথ্য পর্যালোচনা প্রয়োজন" android:layout_column="0" android:layout_width="250sp" android:textColor="#006699" android:textSize="20sp" android:layout_height="wrap_content" android:visibility="invisible"/>
                        <TextView android:id="@+id/VlblCCard" android:text="C-শিশু কার্ড প্রয়োজন" android:layout_column="0" android:layout_width="250sp" android:textSize="20sp" android:textColor="#ff7702" android:layout_height="wrap_content" android:visibility="invisible"/>
                    </LinearLayout>
                    <View
                        android:layout_width="wrap_content"
                        android:layout_height="2dip"
                        android:background="#00CD00" />
                </LinearLayout>


            </ScrollView>
        </LinearLayout>
    </HorizontalScrollView>

